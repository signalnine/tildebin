{
  "apiVersion": "v1",
  "kind": "List",
  "items": [
    {
      "apiVersion": "v1",
      "kind": "Service",
      "metadata": {
        "name": "web-service",
        "namespace": "production"
      },
      "spec": {
        "type": "ClusterIP",
        "clusterIP": "10.96.1.100",
        "ports": [{"name": "http", "port": 80, "protocol": "TCP"}],
        "selector": {"app": "web"}
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Service",
      "metadata": {
        "name": "headless-service",
        "namespace": "production"
      },
      "spec": {
        "type": "ClusterIP",
        "clusterIP": "None",
        "ports": [{"port": 80}],
        "selector": {"app": "headless"}
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Service",
      "metadata": {
        "name": "external-service",
        "namespace": "production"
      },
      "spec": {
        "type": "ExternalName",
        "externalName": "example.com"
      }
    }
  ]
}

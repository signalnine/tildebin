{
  "apiVersion": "v1",
  "kind": "List",
  "items": [
    {
      "apiVersion": "networking.k8s.io/v1",
      "kind": "Ingress",
      "metadata": {
        "name": "web-ingress",
        "namespace": "production"
      },
      "spec": {
        "tls": [
          {
            "hosts": ["example.com"],
            "secretName": "example-tls"
          }
        ],
        "rules": [
          {
            "host": "example.com",
            "http": {
              "paths": [
                {
                  "path": "/",
                  "pathType": "Prefix",
                  "backend": {
                    "service": {
                      "name": "web-service",
                      "port": {"number": 80}
                    }
                  }
                }
              ]
            }
          }
        ]
      },
      "status": {
        "loadBalancer": {
          "ingress": [
            {"ip": "203.0.113.100"}
          ]
        }
      }
    }
  ]
}

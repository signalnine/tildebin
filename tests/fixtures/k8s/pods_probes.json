{
  "apiVersion": "v1",
  "kind": "PodList",
  "items": [
    {
      "metadata": {
        "name": "healthy-pod",
        "namespace": "production"
      },
      "spec": {
        "containers": [
          {
            "name": "app",
            "livenessProbe": {
              "httpGet": {"path": "/health", "port": 8080},
              "initialDelaySeconds": 10,
              "periodSeconds": 10
            },
            "readinessProbe": {
              "httpGet": {"path": "/ready", "port": 8080},
              "initialDelaySeconds": 5,
              "periodSeconds": 5
            }
          }
        ]
      }
    },
    {
      "metadata": {
        "name": "no-probes-pod",
        "namespace": "production"
      },
      "spec": {
        "containers": [
          {
            "name": "app"
          }
        ]
      }
    },
    {
      "metadata": {
        "name": "missing-readiness-pod",
        "namespace": "production"
      },
      "spec": {
        "containers": [
          {
            "name": "app",
            "livenessProbe": {
              "httpGet": {"path": "/health", "port": 8080}
            }
          }
        ]
      }
    }
  ]
}

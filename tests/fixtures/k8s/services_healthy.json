        "selector": {
          "app": "api"
        },
        "ports": [
          {"port": 443, "targetPort": 8443}
        ]
      },
      "status": {
        "loadBalancer": {
          "ingress": [
            {"ip": "203.0.113.10"}
          ]
        }
      }
    }
  ]
